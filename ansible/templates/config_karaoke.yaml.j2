# The qty of songs to be sent to the client on the main song listing page
# You probably dont want to change that
amt_songs_per_page: {{partyhall_karaoke_amt_songs_per_page}}

# The amount of seconds to display a waiting screen before each song
# (The "Now playing:" screen)
pre_play_timer: {{partyhall_karaoke_pre_play_timer}}

# The amount of seconds between when the booth takes a picture (To build the recap video)
# Set to 0 to disable it
# For an average of 3min30 songs, a picture every 5 seconds means 3.6 pictures per second in the final timelapse
unattended_interval: {{partyhall_karaoke_unattended_interval}}